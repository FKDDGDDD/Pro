<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neusoft.mapper.AdmineMapper">
	<select id="findAdmineByName" parameterType="String"	resultType="com.neusoft.po.Admine">
		select * from admine
		where adname = #{name}
	</select>
</mapper>

